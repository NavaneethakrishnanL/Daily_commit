   "source": [
    "def first_non_repeating_char(word):\n",
    "  freq={}\n",
    "  for ch in word:\n",
    "    freq[ch]=freq.get(ch,0)+1\n",
    "  for ch in word:\n",
    "    if freq[ch]==1:\n",
    "      return ch\n",
    "  return None"
   ]
